<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewTitle">
        <attr name="showLeftSrc">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
        <attr name="title" format="string" />
        <attr name="rightText" format="string" />
        <attr name="showRightText">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
        <attr name="rightSrc" format="integer" />
        <attr name="showRightSrc">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
    </declare-styleable>
</resources>